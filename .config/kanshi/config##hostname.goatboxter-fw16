profile mobile {
        output eDP-2 enable
}

profile external_monitors_connected {
        output eDP-2 disable
        output DP-1 enable mode 2560x1440 position 0,0
        output DP-5 enable mode 2560x1440 position 2560,0
}

profile external_monitor_1_connected {
        output eDP-2 disable
        output DP-1 enable mode 2560x1440 position 0,0
}

profile external_monitor_2_connected {
        output eDP-2 disable
        output DP-5 enable mode 2560x1440 position 0,0
}